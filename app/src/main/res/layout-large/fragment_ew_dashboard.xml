        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/showJobsByTech"
            android:gravity="center_horizontal"
            android:orientation="horizontal">
    <ImageView
        android:id="@+id/imgUserInEwDash"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:src="@drawable/user_small"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="@string/jobsByTech"
        android:textSize="@dimen/textsize_large"
        android:textColor="@color/gray_txt"
        android:id="@+id/jobsByTech"
        android:layout_toRightOf="@+id/imgUserInEwDash"
        android:layout_marginLeft="10dp"
        android:gravity="center_vertical"/>
</LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/showJobsInTable"
            android:orientation="vertical">
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/tblInJobsByTech"
                android:layout_weight="2">
            </TableLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/circularGraphInEwDash"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:orientation="vertical">

            </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/showJobsByStatus"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imgClockInEwDash"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:src="@drawable/clock"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="@string/jobsByStatus"
                android:textSize="@dimen/textsize_large"
                android:textColor="@color/gray_txt"
                android:id="@+id/jobsByStatus"
                android:layout_toRightOf="@+id/imgClockInEwDash"
                android:layout_marginLeft="10dp"
                android:gravity="center_vertical"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearGraphInJobstatus"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

        </LinearLayout>
</LinearLayout>
</LinearLayout>